{
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": false,
    "strict": true,
    "noEmit": true,
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ESNext",
    "paths": {
      "@/*": ["../*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["./_generated"]
}